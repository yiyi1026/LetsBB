{
  "name": "letsbb",
  "version": "0.0.0",
  "description": "Example group chat app built with socket.io and react.",
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "build": "webpack --mode=development --config webpack.prod.config.js",
    "local_frontend": "webpack-dev-server --open --hot --inline --config webpack.config.js",
    "server": "node server.dev.js localhost 3000",
    "dev": "npm-run-all -p local_backend local_frontend",
    "start": "node server.js"
  },
  "keywords": [
    "websocket",
    "socket",
    "chat",
    "react",
    "reactjs"
  ],
  "author": "justadudewhohacks",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "babel-loader": "^8.1.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "create-react-app": "3.4.1",
    "express": "^4.17.1",
    "fsevents": "^2.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.2.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.1",
    "webpack": "^4.44.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "babel-plugin-styled-components": "^1.11.1",
    "bcrypt": "^5.0.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "html-webpack-plugin": "^4.4.1",
    "npm-run-all": "^4.1.5",
    "webpack-cli": "^3.3.12",
    "webpack-hot-middleware": "^2.25.0"
  },
  "resolutions": {
    "chokidar": "^3.4.0"
  }
